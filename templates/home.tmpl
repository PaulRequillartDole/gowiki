{{define "title"}}Home{{end}}

{{define "main"}}
    {{range $v := .}}
        <div class="card my-3">
            <div class="card-header">
                <a href="/view/{{ .Title }}">view {{ .Title }}</a>
            </div>
            <div class="card-body">
                {{printf "%s" .Body -}}
            </div>
        </div>
    {{end}}
{{end}}